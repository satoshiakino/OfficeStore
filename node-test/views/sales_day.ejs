<!DOCTYPE html>
<html>
  <head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compotible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <meta name="format-detection" content="telephone=no">
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <header>
      <h1>AkinoStore</h1>
      <nav id="global_navi">
        <ul>
          <li><a href="/mst_menu">マスタ登録・修正</a></li>
          <li><a href="/arrvl_menu">仕入登録・修正</a></li>
          <li><a href="/sales_menu">売上登録・修正</a></li>
          <li class="current"><a href="/sales_check">売上確認</a></li>
          <li><a href="/inventory">在庫確認</a></li>
        </ul>
      </nav>
    </header>
    <div class="wrapper">
      <div align="center">
        <button><a href="/sales_day">日別売上</a></button>
        <button><a href="/sales_week">週別売上</a></button>
        <button><a href="/sales_month">月別売上</a></button>
        <button><a href="/sales_year">年別売上</a></button>
        <table rules="all">
          <tr>
            <th>日付</th>
            <th>売上</th>
          </tr>
          <% resultList.forEach(function(resultItem){ %>
            <tr class="main-list__item">
              <td class="sales_day__trade_day"><%= resultItem.trade_day %></td>
              <td class="sales_day__sales_day"><%= resultItem.sales_day %></td>
            </tr>
          <% }); %>
        </table>
      </div>
    </div>
  </body>
</html>
