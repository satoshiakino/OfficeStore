<!DOCTYPE html>
<html>
  <head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compotible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <meta name="format-detection" content="telephone=no">
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <header>
      <h1>AkinoStore</h1>
      <nav id="global_navi">
        <ul>
          <li><a href="/mst_menu">マスタ登録・修正</a></li>
          <li><a href="/arrvl_menu">仕入登録・修正</a></li>
          <li><a href="/sales_menu">売上登録・修正</a></li>
          <li><a href="/sales_check">売上確認</a></li>
          <li class="current"><a href="/inventory">在庫確認</a></li>
        </ul>
      </nav>
    </header>
    <div class="wrapper">
      <button id="ca_pr_switch"><a href="/invntry_count_result">商品別結果</a></button>
      <div id="sales">
        <table class="sales_trend" rules="all">
          <tr>
            <th>実施日</th>
            <th>カテゴリ名</th>
            <th>在庫金額</th>
          </tr>
          <% resultList.forEach(function(resultItem){ %>
            <tr class="main-list__item">
              <td class="count_date"><%= resultItem.count_date1 %></td>
              <td class="cat_nm"><%= resultItem.cat_nm %></td>
              <td class="stock_value"><%= resultItem.stock_value %></td>
            </tr>
          <% }); %>
        </table>
      </div>
    </div>
  </body>
</html>
